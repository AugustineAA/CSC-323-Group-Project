public class Queue
{
    public Queue nextQueue;
    public Queue prevQueue;
    String tire;
    int numStock;
    int minInv;

    public Queue()
    {
      
    }
    //Queue constructor
    public Queue(String tire,int numStock, int minInv) 
    {
      this.tire=tire;
      this.numStock=numStock;
      this.minInv=minInv;
    }
    //returns letter
    public String getTire()
    {
      return tire;
    }
    //returns value
    public int getNumStock()
    {
      return numStock;
    }
    public int getMinInv()
    {
     return minInv; 
    }
}